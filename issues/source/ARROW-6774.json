                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13260205/comment/17206895",
                    "id": "17206895",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=sietsebb",
                        "name": "sietsebb",
                        "key": "sietsebb",
                        "avatarUrls": {
