                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13369957/comment/17316898",
                    "id": "17316898",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=apitrou",
                        "name": "apitrou",
                        "key": "pitrou",
                        "avatarUrls": {
