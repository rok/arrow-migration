                        "key": "hendrik.makait",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?ownerId=hendrik.makait&avatarId=45134",
                            "24x24": "https://issues.apache.org/jira/secure/useravatar?size=small&ownerId=hendrik.makait&avatarId=45134",
