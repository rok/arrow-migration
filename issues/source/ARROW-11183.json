                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13351083/comment/17267449",
                    "id": "17267449",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=aldanor",
                        "name": "aldanor",
                        "key": "aldanor",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?avatarId=10452",
