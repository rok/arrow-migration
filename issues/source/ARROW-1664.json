                    "id": "16932606",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=mitar",
                        "name": "mitar",
                        "key": "mitar",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?avatarId=10452",
