                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=laurent",
                        "name": "laurent",
                        "key": "laurentgo",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?ownerId=laurentgo&avatarId=18552",
