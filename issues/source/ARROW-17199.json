                },
                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13473378/comment/17571109",
                    "id": "17571109",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=jduong",
                        "name": "jduong",
                        "key": "jduong",
                        "avatarUrls": {
