            "comments": [
                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13378860/comment/17346318",
                    "id": "17346318",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=westonpace",
