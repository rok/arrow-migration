                },
                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13353937/comment/17270480",
                    "id": "17270480",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=bhulette",
