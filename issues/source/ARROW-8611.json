                },
                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13301461/comment/17094910",
                    "id": "17094910",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=npr",
                        "name": "npr",
                        "key": "npr",
                        "avatarUrls": {
