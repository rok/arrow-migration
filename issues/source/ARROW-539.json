                            "24x24": "https://issues.apache.org/jira/secure/useravatar?size=small&ownerId=tebeka&avatarId=16332",
                            "16x16": "https://issues.apache.org/jira/secure/useravatar?size=xsmall&ownerId=tebeka&avatarId=16332",
