                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13492757/comment/17628191",
                    "id": "17628191",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=jorisvandenbossche",
