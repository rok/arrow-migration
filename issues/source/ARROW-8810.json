                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=cboettig",
                        "name": "cboettig",
                        "key": "cboettig",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?avatarId=34058",
