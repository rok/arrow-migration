                    "id": "17502788",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=jabaribooker",
                        "name": "jabaribooker",
                        "key": "JIRAUSER281295",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?avatarId=39936",
