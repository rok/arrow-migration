                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13257150/comment/16931941",
                    "id": "16931941",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=arthur_mac",
