                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13257150/comment/16931920",
                    "id": "16931920",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=andygrove",
                        "name": "andygrove",
                        "key": "andygrove",
                        "avatarUrls": {
