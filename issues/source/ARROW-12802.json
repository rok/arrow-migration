            "comments": [
                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13378678/comment/17345777",
                    "id": "17345777",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=webbp",
