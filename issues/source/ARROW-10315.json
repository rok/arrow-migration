                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13335619/comment/17216554",
                    "id": "17216554",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=mskrzypkowski",
                        "name": "mskrzypkowski",
                        "key": "mskrzypkowski",
                        "avatarUrls": {
                            "48x48": "https://issues.apache.org/jira/secure/useravatar?avatarId=10452",
                            "24x24": "https://issues.apache.org/jira/secure/useravatar?size=small&avatarId=10452",
