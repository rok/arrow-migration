                {
                    "self": "https://issues.apache.org/jira/rest/api/2/issue/13406818/comment/17431118",
                    "id": "17431118",
                    "author": {
                        "self": "https://issues.apache.org/jira/rest/api/2/user?username=reiniervl",
                        "name": "reiniervl",
